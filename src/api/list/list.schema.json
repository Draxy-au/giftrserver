{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "List",
  "description": "The List.",
  "examples": [
      {
          "user_id": 2,
          "name": "Family Christmas List",
          "type": "xmas",          
          "closing": "25/12/2021"
      }
  ],
  "required": [
      "user_id",
      "name",
      "type",
      "closing"
  ],
  "properties": {
      "id": {
          "$id": "#/properties/id",
          "type": "integer",
          "title": "List ID",
          "description": "The List's unique ID"
      },
      "user_id": {
        "$id": "#/properties/user_id",
        "type": "integer",
        "title": "User ID",
        "description": "The User ID of the List owner"
      },
      "name": {
        "$id": "#/properties/name",
        "type": "string",
        "title": "List Name",
        "description": "The List's name."
      },
      "type": {
        "$id": "#/properties/type",
        "type": "string",
        "title": "List Type",
        "description": "The List's type.",
        "examples": [
          "xmas",
          "bday"
        ]
      },
      "closing": {
        "$id": "#/properties/created",
        "type": "string",
        "title": "List Closing Date",
        "description": "The List's closing date."
      },
      "created_at": {
          "$id": "#/properties/created_at",
          "type": "string",
          "title": "List Creation Date.",
          "description": "The creation date of the Listitem."
          
      },
      "updated_at": {
          "$id": "#/properties/updated_at",
          "type": "string",
          "title": "List Last Updated",
          "description": "The date the Listitem was last updated."
      }
  },
  "additionalProperties": false
}